<?xml version='1.0' encoding='utf-8'?>
<program_description>
    <program provider="PUT"
     name="UTA"
     displayName="UTA"
     version="1.0" />
    <documentation>
        <description>This module contains UTA solvers (UTA-G, UTAMP-1, UTAMP-2)</description>
        <contact><![CDATA[Jakub Tomczak (jakub.tomczak@live.com)]]></contact>
        <url></url>
        <reference>KADZINSKI2012541, Corrente2013</reference>
    </documentation>
    <parameters>
		<input id="criteria" name="criteria" displayName="Description of criteria">
			<documentation>
				<description>A list of criteria.</description>
			</documentation>
			<xmcda tag="criteria">
                <![CDATA[	
                    <criteria>
                        <criterion id="[...]" name="[...]">
                            <scale>
                                <quantitative>
                                    <preferenceDirection>[...]</preferenceDirection>
                                </quantitative>
                            </scale>
                        </criterion>
                    </criteria>
                ]]>
            </xmcda>
		</input>

		<input id="criteriaValues" name="criteriaValues" displayName="Criteria values" isoptional="1">
			<documentation>
				<description>
                Options of the criteria. Criteria values with id="characteristicPoints" 
                keeps values of the number of the characteristic points on the criteria.
                </description>
			</documentation>
			<xmcda tag="criteriaValues">
                <![CDATA[	
                    <criteriaValues>
                        <criterionValue>
                            <criterionID>[...]</criterionID>
                            <value>
                                <integer>[...]</integer>
                            </value>
                        </criterionValue>
                        [...]
                    </criteriaValues>
                ]]>
            </xmcda>
		</input>

		<input id="alternatives" name="alternatives" displayName="Alternative list">
			<documentation>
				<description>A list of alternatives. </description>
			</documentation>
			<xmcda tag="alternatives">
                <![CDATA[	
                    <alternatives>
                        <alternative id="[...]" name="[...]"/>
                    </alternatives>
                ]]>
            </xmcda>
		</input>

		<input id="performanceTable" name="performanceTable" displayName="performanceTable">
			<documentation>
				<description>Values of criteria for different alternatives. Values of alternatives should be non-negative real numbers.</description>
			</documentation>
			<xmcda tag="performanceTable">
                <![CDATA[
                    <performanceTable>
                        <alternativePerformances>
                            <alternativeID>[...]</alternativeID>
                            <performance>
                                <criterionID>[...]</criterionID>
                                <value>
                                        <real>[...]</real>
                                </value>
                            </performance>
                        </alternativePerformances>
                    </performanceTable>       
                ]]>
            </xmcda>
		</input>

		<input id="alternativesComparisons" name="alternativesComparisons" displayName="Pariwise comparisons between alternatives" isoptional="1">
			<documentation>
				<description>
                Pariwise comparison relations between alternatives. Alternatives are referred by their IDs.
                Type of the comparison is spcified in 'id' attribute in the 'alternativesComparisons' tag. 
                Available types of comparisons are: 'strong', 'weak', 'indifference'.
                </description>
			</documentation>
			<xmcda tag="alternativesComparisons">
                <![CDATA[
                    <alternativesComparisons>
                        <comparisonType>
                            [...]
                        </comparsionType>
                        <pairs>
                            <pair>
                                <initial>
                                    <alternativeID>[...]</alternativeID>
                                </initial>
                                <terminal>
                                    <alternativeID>[...]</alternativeID>
                                </terminal>
                            </pair>
                        </pairs>
                    </alternativesComparisons>
                ]]>
            </xmcda>
		</input>

        <input id="intensitiesOfPreferences" name="intensitiesOfPreferences" displayName="Intensities of preferences" isoptional="1">
            <documentation>
                <description>Set of comparisons of intensities of preference.</description>
            </documentation>
            <xmcda tag="alternativesComparisons"><![CDATA[
                <alternativesComparisons>
                    <comparisonType>
                        [...]
                    </comparisonType>
                    <pairs>
                        <pair>
                            <initial>
                                <alternativesSet>
                                    <element>
                                        <alternativeID>[...]</alternativeID>
                                    </element>
                                    <element>
                                        <alternativeID>[...]</alternativeID>
                                    </element>
                                </alternativesSet>  
                            </initial>
                            <terminal>
                                <alternativesSet>
                                    <element>
                                        <alternativeID>[...]</alternativeID>
                                    </element>
                                    <element>
                                        <alternativeID>[...]</alternativeID>
                                    </element>
                                </alternativesSet>
                            </terminal>
                        </pair>
                    </pairs>
                </alternativesComparisons>
            ]]></xmcda>
        </input>

        <input id="rankRelatedRequirements" name="rankRelatedRequirements" displayName="Rank related requirements" isoptional="1">
            <documentation>
                <description>Set of rank-related requirements. In other words it is a set of ranges of possible positions in the final ranking for a chosen alternatives.</description>
            </documentation>
            <xmcda tag="alternativesValues"><![CDATA[
                <alternativesValues>
                    <alternativeValue>
                        <alternativeID>%1</alternativeID>
                        <value>
                            <interval>
                                <lowerBound><integer>%2</integer></lowerBound>
                                <upperBound><integer>%3</integer></upperBound>
                            </interval>
                        </value>
                    </alternativeValue>
                    [...]
                </alternativesValues>
            ]]></xmcda>
        </input>

		<input id="methodParameters" name="methodParameters" displayName="method" isoptional="1">
			<documentation>
				<description>Which method should be used as a solver, available options { "uta-g", "utamp-1", "utamp-2"}.</description>
			</documentation>
			<xmcda tag="methodParameters">
                <![CDATA[
                    <methodParameters>
                        <parameter id="methodName" name="Name of the method that should be used in calculations.">
                            <value>
                                <label>%1</label>
                            </value>
                        </parameter>
                    </methodParameters>
                ]]>
            </xmcda>
			<gui status="preferGUI">
				<entry id="%1" type="enum" displayName="Name of method to be used as a solver.">
					<documentation>
						<description>Name of the method to be used as a solver.</description>
					</documentation>
					<items>
						<item id="utag">
							<description>UTA-G</description>
							<value>uta-g</value>
						</item>
						<item id="utamp1">
							<description>UTAMP-2</description>
							<value>utamp-1</value>
						</item>
                        <item id="utamp1">
							<description>UTAMP-1</description>
							<value>utamp-2</value>
						</item>
                        <item id="roruta">
							<description>RORUTA</description>
							<value>roruta</value>
						</item>
					</items>
					<defaultValue>1</defaultValue>
				</entry>
			</gui>
		</input>

        <output id="alternatives" name="alternatives" displayName="Alternatives global utility values">
            <documentation>
                <description>Global utility values for all alternatives.</description>
            </documentation>
            <xmcda tag="alternativesValues"/>
        </output> 
              
        <output id="valueFunctions" name="valueFunctions" displayName="Utility functions elicited from a model">
            <documentation>
                <description>Elicited value functions.</description>
            </documentation>
            <xmcda tag="criteriaFunctions"/>
        </output>

        <output id="best-ranking" name="best-ranking" displayName="best ranking" optional="1">
            <documentation>
                <description>The best possible ranking positions that may be attained by the alternatives taking into consideration all value functions that are compatible with the preference information.</description>
            </documentation>
            <xmcda tag="alternativesValues"><![CDATA[
                <alternativesValues id=[...]>
                    <alternativeValue>
                        <alternativeID>[...]</alternativeID>
                        <value>
                            <integer>[...]</integer>
                        </value>
                        </alternativeValue>
                    [...]
                </alternativesValues>
            ]]></xmcda>
        </output>

        <output id="worst-ranking" name="worst-ranking" displayName="worst ranking" optional="1">
            <documentation>
                <description>The worst possible ranking positions that may be attained by the alternatives taking into consideration all value functions that are compatible with the preference information.</description>
            </documentation>
            <xmcda tag="alternativesValues"><![CDATA[
                <alternativesValues id=[...]>
                    <alternativeValue>
                        <alternativeID>[...]</alternativeID>
                        <value>
                            <integer>[...]</integer>
                        </value>
                        </alternativeValue>
                    [...]
                </alternativesValues>
            ]]></xmcda>
        </output>

        <output id="necessaryRelations" name="necessaryRelations" displayName="Necessary relations from roruta method"  isoptional="1">
            <documentation>
                <description>A list of all necessary weak preference relations in the set of alternatives.</description>
            </documentation>
            <xmcda tag="alternativesComparisons"><![CDATA[
                <alternativesComparisons mcdaConcept="necessaryRelations" id="%1">
                    <pairs>
                        <pair>
                        <initial>
                            <alternativeID>%2</alternativeID>
                        </initial>
                        <terminal>
                            <alternativeID>%3</alternativeID>
                        </terminal>
                        </pair>
                        [...]
                    </pairs>
                </alternativesComparisons>
            ]]></xmcda>
        </output>

        <output id="possibleRelations" name="possibleRelations" displayName="Possible relations from roruta method" isoptional="1">
            <documentation>
                <description>A list of all possible weak preference relations in the set of alternatives.</description>
            </documentation>
            <xmcda tag="alternativesComparisons"><![CDATA[
                <alternativesComparisons mcdaConcept="possibleRelations" id=%1>
                    <pairs>
                        <pair>
                        <initial>
                            <alternativeID>%1</alternativeID>
                        </initial>
                        <terminal>
                            <alternativeID>%2</alternativeID>
                        </terminal>
                        </pair>
                        [...]
                    </pairs>
                </alternativesComparisons>
            ]]></xmcda>
        </output>

		<output id="messages" name="messages" displayName="messages">
			<documentation>
				<description>A list of messages generated by the algorithm.</description>
			</documentation>
			<xmcda tag="methodMessages" />
		</output>
    </parameters>
</program_description>